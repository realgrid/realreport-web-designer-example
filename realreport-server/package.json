{
    "name": "realreport-server",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "check": "tsc --noEmit",
        "lint": "eslint",
        "clean": "prettier . --write",
        "migrate:new": "sh -c 'knex migrate:make create-$1-table' _",
        "migrate": "dotenv -e dev.env -- knex migrate:latest",
        "seed": "dotenv -e dev.env -- knex seed:run",
        "init": "npm run migrate & npm run seed",
        "start": "dotenv -e dev.env -- ts-node-dev -r tsconfig-paths/register -- ./src/app.ts",
        "start:db": "docker compose up -d",
        "stop:db": "docker compose stop",
        "destroy:db": "docker compose down"
    },
    "dependencies": {
        "@types/multer": "^1.4.11",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "ejs": "^3.1.10",
        "express": "^4.19.2",
        "http-errors": "~1.6.3",
        "knex": "^3.1.0",
        "morgan": "~1.9.1",
        "multer": "^1.4.5-lts.1",
        "pg": "^8.12.0",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.2.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.3.0",
        "@types/cookie-parser": "^1.4.7",
        "@types/cors": "^2.8.17",
        "@types/eslint": "^8.56.10",
        "@types/eslint__js": "^8.42.3",
        "@types/eslint-config-prettier": "^6.11.3",
        "@types/express": "^4.17.21",
        "@types/express-ejs-layouts": "^2.5.4",
        "@types/morgan": "^1.9.9",
        "@types/prettier": "^3.0.0",
        "dotenv-cli": "^7.4.2",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "prettier": "^3.1.1",
        "typescript": "^5.4.5",
        "typescript-eslint": "7.11.0"
    }
}
